{
  "name": "DavidTheme",
  "author": "David Cruz",
  "dark": true,
  "editorScheme": "/DavidTheme.xml",
  "colors": {
    "black": "#000000",
    "yellow": "#FFB900",
    "greyDot60": "#666666",
    "greyDot95": "#242220",
    "white": "#FFFFFF"
  },
  "ui": {
    "*": {
      "background": "black",
      "foreground": "white",
      "inactiveBackground": "greyDot95",
      "borderColor": "yellow",
